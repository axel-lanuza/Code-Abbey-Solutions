/*****************************************************************************
INSTRUCTIONS
Input data will give you exactly 300 numbers in a single line.
Answer should contain maximum and minimum of these values, separated by space.

Example:

input data:
1 3 5 7 9 11 ... 295 297 299 300 298 296 ... 12 10 8 6 4 2

answer:
300 1
*****************************************************************************/

//input data from Code Abbey
var arrStr = "8770 23889 -42716 -31234 -32118 -76861 -24352 -13820 -52771 -19685 -8442 -32843 -11071 -16051 64859 27726 -36964 -74870 -59471 -15475 -66689 -41524 -61527 42231 -26878 -25812 -52661 -46193 10821 -14891 7653 -60409 -71001 44936 -11644 -23120 48075 44004 43060 75304 -55680 -45381 -37539 13248 18567 -52680 -39026 61602 -47550 -18497 -33872 -34240 19979 -15400 -72009 73100 38788 -44671 -53092 -30391 20438 34561 -10800 29436 -503 57556 -73684 -32427 21560 49376 -37123 45880 -76005 5338 -20872 22561 32658 20102 4164 65107 -78395 50291 -49133 21583 -45108 -41142 14684 73680 -5813 41592 -36710 -65375 -3847 32489 44061 75650 10045 50376 -36777 -48394 19753 6100 77485 23747 -68562 -23386 -33691 44096 76715 50473 29203 78320 20765 60070 19904 55656 -61072 -45411 49337 13115 76180 -67373 27739 -7667 45115 -8199 -12016 -24839 -37822 31206 6767 61930 -42693 4253 5677 -31255 60867 51985 -67159 57582 22458 42044 55903 -36777 22114 -4193 -61120 41041 30395 68216 -25844 26576 -79156 -78104 -61090 45959 -6304 6893 -58880 35874 -41900 27887 17804 -4594 -47859 -56519 44150 -66992 75466 56991 70590 17924 19035 46493 61147 -38851 -37699 -79972 -77809 72696 68244 -23653 19272 69087 -21757 38181 35046 51939 -34925 56166 7813 3174 4053 -54383 78579 36194 -30902 42730 49201 -35436 19722 39792 62488 -41243 6285 43636 -94 48586 43663 2096 41282 31907 58443 -19445 20994 -43313 -61263 -23959 -71374 -16189 -47793 16439 66985 36260 42055 65564 -7546 -68847 28295 -38344 -24283 -31983 -78552 -41795 6774 7733 -78159 -73320 -23681 45504 8776 -62398 -2589 -12781 -1843 -61594 23906 16893 -5554 32531 -79295 26652 -31030 67689 -17087 -68974 53254 55366 -57821 1549 -62978 -2105 49566 -61530 36100 -23660 26202 37941 -16981 -77478 3445 71795 -59876 -79144 -20985 18280 -60738 -77079 -44827 13707 35451 -44123 -39640 -75578 -56434 23272 -64552 76819 -1361 -42374 -1631 15661 35521 -32065 34130 -8378 24275 -19667 -50437 -72705";

//change the string of numbers into an array, separated by commas.
var arr = arrStr.split(" "); 

//change the array of strings into an array of integers
for (var i=0; i<arr.length; i++){ 
	arr[i] = parseInt(arr[i], 10);
} 

//scan the array for the numbers with the greatest and least values
var maximumAndMinimum = function (arr) {
	var greatest = arr[0]; //initialize the greatest value as the zeroth position in the array
    var least = arr[0];    //initialize the least value as the zeroth position in the array
	for (var i = 0; i < arr.length; i++){
        if (arr[i] > greatest) {
            greatest = arr[i];
        } else if (arr[i] < least) {
            least = arr[i];
        };        
	}; console.log(greatest.toString() + " " + least.toString());
};

maximumAndMinimum(arr);
